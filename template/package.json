{
  "name": "ferline",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "react-native run-android --variant=release",
    "install:clean": "rm -rf node_modules && yarn && yarn clean:android && yarn clean:ios && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:ios": "cd ios && rm -rf Podfile.lock && rm -rf Pods && cd ..",
    "report:android": "cd android && ./gradlew signingReport && cd .. && pod-install",
    "pod-install": "cd ios && pod install --repo-update && cd ..",
    "pod-install:new-arch": "./ RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\" .",
    "lint": "eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "jest"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@gorhom/paper-onboarding": "^1.2.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react-native-vector-icons": "^6.4.13",
    "moti": "^0.25.1",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-animated-pagination-dots": "^0.1.73",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-pager-view": "^6.2.0",
    "react-native-reanimated": "^2.16.0",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
